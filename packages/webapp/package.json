{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "author": "chenc",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.conf.ts",
    "build": "pnpm run clean && cross-env NODE_ENV=production webpack --config config/webpack.prod.conf.ts",
    "analyzer": "cross-env ANALYZER=true webpack --config config/webpack.prod.conf.ts",
    "clean": "rimraf ../api/client",
    "prebuild": "pnpm run clean",
    "lint-staged": "lint-staged",
    "lint-staged:tsx": "tsc --noEmit && eslint --fix 'src/**/**/*.{js,ts,tsx}'",
    "prettier": "prettier --write src/**/**/*.tsx",
    "cy:run": "cypress run"
  },
  "browserslist": [
    "cover 99.5% in CN"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.9.1",
    "axios": "^1.5.0",
    "core-js": "^3.32.2",
    "postcss": "^8.4.29",
    "postcss-loader": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "typed.js": "^2.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@types/html-webpack-plugin": "^3.2.7",
    "@types/node": "^20.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-helmet": "^6.1.6",
    "@types/webpack": "^5.28.2",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "cypress": "^13.2.0",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended": "^13.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "thread-loader": "^4.0.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "lint-staged": {
    "src/**/**/*.tsx": [
      "prettier --write",
      "git add"
    ],
    "**/**/*.tsx": [
      "prettier --write",
      "git add"
    ],
    "**/*.tsx": "npm run lint-staged:tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
